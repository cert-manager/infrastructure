docker run --rm -it golang:1.16-buster 
set -euo
git clone https://github.com/jetstack/cert-manager.git
git clone https://github.com/cert-manager/test-infra.git
apt-get update && apt-get install -y python3 unzip
curl "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"
unzip awscliv2.zip
./aws/install
aws --version
curl -o aws-iam-authenticator https://amazon-eks.s3.us-west-2.amazonaws.com/1.19.6/2021-01-05/bin/linux/amd64/aws-iam-authenticator
chmod +x ./aws-iam-authenticator
mkdir -p $HOME/bin && cp ./aws-iam-authenticator $HOME/bin/aws-iam-authenticator && export PATH=$PATH:$HOME/bin
aws-iam-authenticator help
cd test-infra/aws
curl -O https://releases.hashicorp.com/terraform/1.0.3/terraform_1.0.3_linux_amd64.zip
unzip terraform_1.0.3_linux_amd64.zip 
mv terraform /usr/local/bin/
terraform init
terraform apply -auto-approve
cd ..
cd ..
go get github.com/onsi/ginkgo/ginkgo
go get github.com/onsi/gomega/...
cd cert-manager
./devel/run-e2e.sh --ginkgo.focus "Public ACME Server HTTP01 Issuer" --acme-server-url=https://acme-staging-v02.api.letsencrypt.org/directory --ingress-controller-domain=aws.e2e-tests.cert-manager.io --testing-acme-email=arshsharma461+acme-tests-1@gmail.com --kubernetes-config=/home/arsh/projects/forks/test-infra-1/aws/kubeconfig_cert-manager-cluster;